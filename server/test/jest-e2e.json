{
  "collectCoverage": true,
  "moduleFileExtensions": [
      "js",
      "json",
      "ts"
  ],
  "rootDir": "./",
  "modulePaths":["<rootDir>"],
  "moduleNameMapper": {
    "src/(.*)": "<rootDir>/src/$1",
    "test/(.*)": "<rootDir>/__tests__/$1"
  },
  "testRegex": ".spec.ts$",
  "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
  },
  "coverageDirectory": "../coverage",
  "testEnvironment": "node"
}
