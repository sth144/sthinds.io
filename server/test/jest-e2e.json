{
  "collectCoverage": true,
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "../src",
  "modulePaths": [
    "<rootDir>"
  ],
  "moduleNameMapper": {
    "src/(.*)": "<rootDir>/src/$1",
    "test/(.*)": "<rootDir>/__tests__/$1"
  },
  "testRegex": ".*\\.spec\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverageFrom": [
    "**/*.(t)s"
  ],
  "coverageDirectory": "../../test/reports/coverage/server_e2e",
  "testEnvironment": "node"
}
